{
    "source": {
        "include":["src",
        "src/components"
        ],
        "inclucePattern": ".js$",
        "excludePattern": "(node_modules/|docs)",
        "includePattern": "\\.(vue|js)$"
    },
    "plugins": [
        "plugins/markdown",
        "node_modules/jsdoc-vuejs"
    ],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts":{
        "recurse": true,
        "destination": "./docs/"
    }
}